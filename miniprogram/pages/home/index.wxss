/* pages/home/index.wxss */
page {
  opacity: 0;
  animation: show 500ms 500ms forwards;
}

.container {
  min-height: 100vh;
  transition: 500ms ease-in-out;
  transform: translate3d();
  transform-origin: 100% 0 0;
}

.headerContainer {
  display: flex;
  position: fixed;
  margin: 90rpx 60rpx 30rpx 50rpx;
  z-index: 5;
  width: calc(100% - 100rpx);
}

.sidebar {
  position: fixed;
  width: 750rpx;
  height: 100vh;
  z-index: 6;
  transition: 500ms ease-in-out;
}

.sidebar .mask {
  position: absolute;
  width: 750rpx;
  height: 100vh;
  background: #00000030;
  opacity: 0;
}

.sidebar .container {
  background: #ffffff;
  position: absolute;
  width: 450rpx;
  padding-top: 10vh;
  height: 90vh;
  color: #313131;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.sidebar .user .name {
  font-size: 50rpx;
  max-width: 300rpx;
  overflow: scroll;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.btns {
  display: flex;
  margin-bottom: 30rpx;
}

.sidebar .count {
  display: flex;
  flex-direction: row;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #00000010;
}

.sidebar .setting {
  font-size: 30rpx;
}

.sidebar .setting .item {
  display: flex;
  color: #00000060;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #00000005;
}

.sidebar .heatTip {
  position: absolute;
  bottom: env(safe-area-inset-bottom);
  margin-bottom: 20px;
  padding: 20rpx 30rpx;
  background: #07C160;
  border-radius: 50rpx;
  color: #ffffff;
  right: 50rpx;
  width: fit-content;
  opacity: 0;
  animation: show 300ms forwards;
}

.sidebar .openWeview {
  padding: 20rpx 0;
  color: #07C160;
}

.sidebar .setting .item .value {
  margin-left: auto;
  color: #07C160;
}

.sidebar .user {
  display: flex;
  padding-bottom: 30rpx;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #00000010;
}

.sidebar .user .badge {
  margin-left: 20rpx;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
  background-color: #f5f5f5;
}

.sidebar .heatMap {
  display: flex;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #00000010;
}

.sidebar .date {
  position: absolute;
  bottom: env(safe-area-inset-bottom);
  color: #00000050;
  width: 90%;
  text-align: center;
}

.sidebar .heatMap .day {
  font-size: 20rpx;
  margin: 20rpx 0 10rpx 0;
  display: flex;
  flex-direction: column;
}

.sidebar .heatMap .map {
  display: flex;
  margin: 10rpx;
  flex-direction: row-reverse;
}

.sidebar .heatMap .map .mapColumn {
  margin: 0 5rpx;
}

.sidebar .heatMap .map .mapColumn .block {
  width: 25rpx;
  height: 25rpx;
  background-color: #f5f5f5;
  margin-top: 10rpx;
  border-radius: 5rpx;
}

.sidebar .count .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #00000060;
  flex: 1 1 0;
}

.sidebar .count .item .num {
  font-size: 50rpx;
  font-family: 'Smiley Sans Oblique';
}

.shareImg_block {
  width: 100%;
  height: 100vh;
  background-color: #00000020;
  backdrop-filter: blur(30rpx);
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 6;
  overflow: hidden
}

.shareImg {
  width: 600rpx;
  height: 800rpx;
  background-color: #cccccc;
  border-radius: 30rpx;
  opacity: 0;
  transform: rotateY(90deg);
  animation: shareImg 300ms 300ms ease-in-out forwards;
}

.line {
  height: 1rpx;
  width: 100%;
  background-color: #00000020;
  margin: 20rpx 0;
}

.Tips {
  overflow: scroll;
  width: 500rpx;
  height: 700rpx;
  display: flex;
  flex-direction: column;
  padding: 50rpx;
  background-color: #f5f5f5;
  border-radius: 30rpx;
  opacity: 0;
  transform: rotateY(90deg);
  animation: shareImg 300ms 300ms ease-in-out forwards;
}

.Tips::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}


.Tip_title {
  margin: 10rpx 0;
  color: #07C160;
}

.Tip_p {
  margin: 10rpx 0;
  color: #000000;
}

.Tip_pic_1 {
  flex-shrink: 0;
  margin: 10rpx 0;
  width: 100%;
  height: 225rpx;
  background-repeat: no-repeat;
  background-size: contain;
  background-image: url('data:image/svg+xml;base64,');
}

.memo-link-text {
  color: #07C160;
}

.header {
  display: flex;
  align-items: center;
  color: #cccccc;
  background: #f5f5f580;
  backdrop-filter: blur(30px);
  padding: 5rpx 20rpx;
  border-radius: 99rpx;
  opacity: 0;
  animation: show 300ms 100ms forwards;
}

.icon {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 1rpx solid #cccccc50;
  margin-right: 20rpx;
}

.canvas {
  position: absolute;
  left: 750rpx;
  width: 0;
  height: 0;
}

@keyframes shareImg {
  from {
    opacity: 0;
    transform: rotateY(90deg);
  }

  to {
    opacity: 1;
    transform: rotateY(0deg);
  }
}


@import 'index-wxa-auto-dark.wxss'
/* pages/home/index.wxss */
page {
  opacity: 0;
  animation: show 500ms 500ms forwards;
}

.editBtn {
  width: 40rpx;
  height: 40rpx;
  filter: grayscale(1);
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.getUserInfoBtn {
  position: absolute;
  background-color: #ffffff20;
  padding: 20rpx 30rpx;
  border: 1rpx solid #00000010;
  color: #07C160;
  backdrop-filter: blur(20px);
  border-radius: 20rpx;
}

.pinFolder {
  padding: 20rpx 32rpx;
  border-bottom: 1rpx solid #00000010;
  background: #ffffff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: rgb(66, 65, 63);
}

.arrowIcon {
  animation-duration: 300ms;
  transition-duration: 300ms;
  width: 30rpx;
  height: 30rpx;
  background-image: url('data:image/svg+xml;base64,');
  background-size: cover;
}

.colorDot {
  position: absolute;
  background: #07C160;
  /* z-index: 1; */
  filter: blur(80px);
  top: 80rpx;
}


.colorDot1 {
  width: 150rpx;
  height: 50rpx;
  background: #07C160;
  left: 200rpx;
}

.colorDot2 {
  width: 150rpx;
  height: 50rpx;
  background: #ffca27;
  left: 400rpx;
}

.container {
  min-height: 100vh;
  transition: 500ms ease-in-out;
  transform: translate3d();
  transform-origin: 100% 0 0;
}

.headerContainer {
  display: flex;
  position: fixed;
  margin: 90rpx 30rpx 30rpx 30rpx;
  z-index: 5;
  width: calc(100% - 60rpx);
  overflow-y: scroll;
}

.sidebar {
  position: fixed;
  width: 750rpx;
  height: 100vh;
  z-index: 6;
  transition: 500ms ease-in-out;
}

.sidebar .mask {
  position: absolute;
  width: 750rpx;
  height: 100vh;
  background: #00000030;
  opacity: 0;
  backdrop-filter: blur(5px);
}

.sidebar .container {
  background: #ffffff;
  position: absolute;
  width: 450rpx;
  padding-top: 10vh;
  height: 90vh;
  color: #313131;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.sidebar .user .name {
  font-size: 50rpx;
  max-width: 300rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.sidebar .user .avatar {
  width: 60rpx;
  height: 60rpx;
  border-radius: 999px;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.btns {
  display: flex;
  margin-bottom: 30rpx;
}

.sidebar .count {
  display: flex;
  flex-direction: row;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #00000010;
}

.sidebar .setting {
  font-size: 30rpx;
}

.sidebar .setting .item {
  display: flex;
  color: #00000060;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #00000005;
}

.sidebar .heatTip {
  position: absolute;
  bottom: env(safe-area-inset-bottom);
  margin-bottom: 20px;
  padding: 20rpx 30rpx;
  background: #07C160;
  border-radius: 50rpx;
  color: #ffffff;
  right: 50rpx;
  width: fit-content;
  opacity: 0;
  animation: show 300ms forwards;
}

.sidebar .openWeview {
  padding: 20rpx 0;
  color: #07C160;
}

.sidebar .archived {
  padding: 20rpx 0;
  color: #07C160;
}

.sidebar .setting .item .value {
  margin-left: auto;
  color: #07C160;
}

.createMemoBtn {
  position: fixed;
  bottom: 100rpx;
  right: 0rpx;
  padding: 20rpx 30rpx;
  background-color: #ffffff;
  border: 1rpx solid #00000010;
  border-radius: 20rpx 0 0 20rpx;
  font-weight: bolder;
  transition-duration: 300ms;
  display: flex;
  align-items: center;
  gap: 12rpx;
}

.dotRed {
  width: 30rpx;
  height: 30rpx;
  background-image: url(data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMTIiIGhlaWdodD0iMTIiIHZpZXdCb3g9IjAgMCAxMiAxMiIgZmlsbD0ibm9uZSIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj48Y2lyY2xlIGN4PSI2IiBjeT0iNiIgcj0iMyIgZmlsbD0iI0VFNDYzNyIvPjxjaXJjbGUgY3g9IjYiIGN5PSI2IiByPSI1LjUiIHN0cm9rZT0iI0VFNDYzNyIvPjwvc3ZnPg==);
  background-size: cover;
  opacity: 0;
  animation: show alternate-reverse ease-in 2s infinite;
}

.createMemoBtn:active {
  transform: scale(0.9);
}

.sidebar .user {
  display: flex;
  padding-bottom: 30rpx;
  /* justify-content: center; */
  align-items: center;
  border-bottom: 1rpx solid #00000010;
  position: relative;
}

.sidebar .user .badge {
  margin-left: 20rpx;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
  background-color: #f5f5f5;
  font-weight: bold;
  font-family: 'Smiley Sans Oblique';
}

.sidebar .heatMap {
  display: flex;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #00000010;
}

.sidebar .date {
  position: fixed;
  bottom: env(safe-area-inset-bottom);
  color: #00000050;
  width: 450rpx;
  text-align: center;
}

.sidebar .heatMap .day {
  font-size: 20rpx;
  margin: 20rpx 0 10rpx 0;
  display: flex;
  flex-direction: column;
}

.sidebar .heatMap .map {
  display: flex;
  margin: 10rpx;
  flex-direction: row-reverse;
}

.sidebar .heatMap .map .mapColumn {
  margin: 0 5rpx;
}

.sidebar .heatMap .map .mapColumn .block {
  width: 25rpx;
  height: 25rpx;
  background-color: #f5f5f5;
  margin-top: 10rpx;
  border-radius: 5rpx;
}

.sidebar .count .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #00000060;
  flex: 1 1 0;
}

.sidebar .count .item .num {
  font-size: 50rpx;
  font-family: 'Smiley Sans Oblique';
}

.shareImg_block {
  width: 100%;
  height: 100vh;
  background-color: #00000020;
  backdrop-filter: blur(30rpx);
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 6;
  overflow: hidden
}

.shareImg {
  width: 600rpx;
  height: 800rpx;
  background-color: #cccccc;
  border-radius: 30rpx;
  opacity: 0;
  transform: rotateY(90deg);
  animation: shareImg 300ms 300ms ease-in-out forwards;
}

.line {
  height: 1rpx;
  width: 100%;
  background-color: #00000020;
  margin: 20rpx 0;
}

.Tips {
  overflow: scroll;
  width: 500rpx;
  height: 700rpx;
  display: flex;
  flex-direction: column;
  padding: 50rpx;
  background-color: #f5f5f5;
  border-radius: 30rpx;
  opacity: 0;
  transform: rotateY(90deg);
  animation: shareImg 300ms 300ms ease-in-out forwards;
}

.Tips::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}


.Tip_title {
  margin: 10rpx 0;
  color: #07C160;
}

.Tip_p {
  white-space: pre-line;
  line-height: 2;
  font-size: 28rpx;
}

